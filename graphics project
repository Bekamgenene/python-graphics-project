from cs1graphics import *
from random import randint
canvas= Canvas(1320, 900)
canvas.setBackgroundColor((190, 234, 245))

world = Layer()
road = Rectangle(4000, 200, Point(700, 650))
world.add(road)
road.setFillColor((48, 57, 102))
road.setBorderWidth(0)
road.setDepth(40)
### ========================== make road realistic ==============###
road = Rectangle(4000, 10, Point(700, 530))
world.add(road)
road.setFillColor((30, 20, 21))
road.setBorderWidth(0)
road_colors = [Rectangle(100, 10, Point(i*200, 630)) for i in range(8)]
road.setDepth(40)

for i in road_colors:
    i.setFillColor("white")
    i.setBorderWidth(0)
    i.setDepth(40)
    world.add(i)

road1 = Rectangle(4000, 150, Point(700, 360))
world.add(road1)
