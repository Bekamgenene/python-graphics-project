#===============================train============================#
train=Layer()
engine=Polygon(Point(70, 80), Point(90, 50), Point(250, 50), Point(250, 110), Point(80, 110), Point(70, 100))
engine.setFillColor("deepskyblue")
line1=Path(Point(85,60),Point(250,60))
line1.setDepth(30)
colER=Polygon(Point(70, 100), Point(70, 90), Point(250, 90), Point(250, 100))
colER.setFillColor("red")
colER.setDepth(30)
colEY=Polygon(Point(150, 85), Point(160, 60), Point(250, 60), Point(250, 70), Point(195, 70), Point(190, 85))
colEY.setFillColor("white")
colEY.setDepth(30)



conte=Rectangle(50,10,Point(275,100))
conte.setBorderWidth(4)
train.add(conte)
text=Text("Ethiopia train",15,Point(135,80))

text.setDepth(25)
text.setFontColor("black")
train.add(text)

goma=Layer()
Wheel=Layer()
wheelE1=Circle(10, Point(100, 115))
wheelE1.setFillColor("Black")

cent=Circle(3,Point(100,116))
cent.setFillColor("cyan")
cent.setDepth(25)
Wheel.add(cent)

Wheel.add(wheelE1)

wheelpath=Path(Point(100,116),Point(100,126))
wheelpath.setBorderColor("deepskyblue")
wheelpath.setDepth(20)
wheelpath.setBorderWidth(2)
goma.add(wheelpath)
train.add(Wheel)

WheelE2=Wheel.clone()
WheelE2.move(45,0)
train.add(WheelE2)

wheelpath2=wheelpath.clone()
wheelpath2.move(45,0)
goma.add(wheelpath2)


diago=Path(Point(100,116),Point(145,116))
diago.setBorderWidth(3)
diago.setDepth(51)
train.add(diago)


