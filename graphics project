rode =Layer()
k1=Rectangle(79,600,Point(649,450))
k1.setFillColor("black")
k1.setBorderColor("black")
rode.add(k1)
l1=k1.clone()
l1.rotate(6.35)
l1.moveTo(617,453)
rode.add(l1)
m1=k1.clone()
m1.rotate(-6.35)
m1.moveTo(683,453)
rode.add(m1)
