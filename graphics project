#=============================the_real_grass==================#
real_grass  = Polygon(Point(-5, 0), Point(0, -30), Point(5, 0))
real_grass.setFillColor("OliveDrab")
hold_grass = Layer()
hold_grass.add(real_grass)
world.add(hold_grass)
real_grass.setBorderWidth(00)
hold_grass.moveTo(10, 520)
hold_grass.setDepth(4)
grasses = [hold_grass.clone() for i in range(200)]
grasses2 = [hold_grass.clone() for i in range(200)]
for i in range(len(grasses)):
    grasses[i].moveTo(i*20, 540)
    grasses2[i].moveTo(i*20+10, 550)
    world.add(grasses[i])
    world.add(grasses2[i])
