### ============= cse school bus =============================###

car=Layer()
body=Rectangle(152,50,Point(50,575))
body.setFillColor('yellow')
car.add(body)
n=Layer()
spokyo=Polygon(Point(250,500),Point(290,500),Point(290,520),Point(280,520),Point(280,505),Point(250,505))
spokyo.setFillColor('black')
spokyo.moveTo(110,552)
n.add(spokyo)
car.add(n)
mx=Text(" CSE  SCHOOL BUS",12,Point(50,580))
mx.setFontColor("black")
mx.setDepth(45)
car.add(mx)
window=Rectangle(20,20,Point(-12,562))
window.setFillColor("white")
car.add(window)
w1=window.clone()
w1.setFillColor("white")
w1.moveTo(12,562)
car.add(w1)
w2=window.clone()
w2.setFillColor("white")
w2.moveTo(37,562)
car.add(w2)
w3=window.clone()
w3.setFillColor("orange")
w3.setHeight(45)
w3.moveTo(60,574)
car.add(w3)
w4=window.clone()
w4.setFillColor("white")
w4.moveTo(83,562)
car.add(w4)
w5=window.clone()
w5.setFillColor("white")
w5.moveTo(106,562)
car.add(w5)
tyre1=Circle(10,Point(88,600))
tyre1.setFillColor("black")
car.add(tyre1)
